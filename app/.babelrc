{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": "last 2 version",
            "ie": "11"
          },
          "useBuiltIns": "usage"
        }
      }
    ],
    "@zeit/next-typescript/babel"
  ],
  "plugins": [
    "styled-components",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./"
        }
      }
    ]
  ]
}
